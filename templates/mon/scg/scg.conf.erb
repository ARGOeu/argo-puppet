[GENERAL]
sensu_url = <%= @sensu_url %>
sensu_token = <%= @sensu_token %>
webapi_url = <%= @webapi_url %>

<% @tenant_sections.each do |key, value| -%>
[<%= key %>]
poem_url = <%= value['poem_url']%>
poem_token = <%= value['poem_token']%>
webapi_token = <%= value['webapi_token']%>
<% if value['topology_filter'] -%>
topology_filter = <%= value['topology_filter'] %>
<% end -%>
<% if value['topology_groups_filter'] -%>
topology_groups_filter = <%= value['topology_groups_filter'] %>
<% end -%>
<% if value['topology_endpoints_filter'] -%>
topology_endpoints_filter = <%= value['topology_endpoints_filter'] %>
<% end -%>
metricprofiles = <%= value['metricprofiles']%>
<% if value['topology'] -%>
topology = <%= value['topology'] %>
<% end -%>
<% if value['secrets'] -%>
secrets = <%= value['secrets'] %>
<% end -%>
publish = <%= value['publish'] %>
<% if value['publish'] == 'true' -%>
publisher_queue = <%= value['publisher_queue'] %>
<% end -%>
<% if value['subscription'] -%>
subscription = <%= value['subscription'] %>
<% end -%>
<% if value['agents_configuration'] -%>
agents_configuration = <%= value['agents_configuration'] %>
<% end -%>
<% if value['skipped_metrics'] -%>
skipped_metrics = <%= value['skipped_metrics'] %>
<% end -%>
<% if value['namespace'] -%>
namespace = <%= value['namespace'] %>
<% end -%>

<% end -%>